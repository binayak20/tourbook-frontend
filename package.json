    "i18next": "^21.8.3",
    "i18next-http-backend": "^1.4.0",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "polished": "^4.2.2",
    "react": "^18.1.0",
    "react-access-boundary": "^2.1.4",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.16.9",
    "react-query": "^3.39.1",
