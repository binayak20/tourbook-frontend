  "scripts": {
    "dev": "vite",
    "dev:widget": "concurrently \"cross-env WIDGET=1 vite build --watch\" \"vite preview\"",
    "build": "tsc && vite build && tsc && cross-env WIDGET=1 vite build",
    "preview": "vite preview",
    "format": "prettier -c -w src/**/*",
    "format:check": "prettier -c src/**/*",
    "lint": "tsc && eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install"
  },
